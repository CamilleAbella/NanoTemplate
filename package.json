{
  "name": "nano-template",
  "author": "ghom",
  "license": "ISC",
  "version": "v1.8.4-alpha",
  "description": "Discord bot in TypeScript.",
  "main": "dist/index.js",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false,
    "useTabs": false
  },
  "scripts": {
    "prettier": "prettier src scripts package.json nano.config.json nano.config.schema.json readme.md tsconfig.json --write",
    "build": "git rm -r --ignore-unmatch -f ./dist && npm run prettier && tsc",
    "start": "npm run build && node .",
    "precommit": "node scripts/pre-commit && npm run prettier && git add *",
    "commit": "git commit -m \"patch to v1.8.4-alpha\"",
    "push": "npm run commit && git push origin master"
  },
  "keywords": [
    "discord.js",
    "bot",
    "typescript",
    "handler",
    "command-handler"
  ],
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/ws": "^7.2.6",
    "prettier": "^2.0.5",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "better-sqlite3": "7.1.0",
    "chalk": "4.1.0",
    "discord.js": "12.3.1",
    "dotenv": "8.2.0",
    "enmap": "5.6.1",
    "querystring": "0.2.0"
  }
}
