{
  "bot": {
    "prefix": "nano ",
    "clientOptions": {
      "disableMentions": "everyone"
    },
    "embedTemplates": {
      "default": {
        "author": {
          "name": "{{ Globals.bot.name }}",
          "iconURL": "{{ Globals.client.user.displayAvatarURL() }}"
        },
        "color": "#f0ddb1"
      },
      "success": {
        "author": {
          "name": "Success!  (ง `ω´)۶"
        },
        "image": "https://64.media.tumblr.com/7ff7a1fe3721ad25fbc7637e7cf5176d/tumblr_n6feedzYkM1rbnx7io1_500.gif",
        "color": "#70ff96"
      },
      "error": {
        "author": {
          "name": "Error...  ⊂(⊙д⊙)つ"
        },
        "image": "https://i.kym-cdn.com/photos/images/original/000/652/857/fca.gif",
        "color": "#ff5959"
      },
      "log": {
        "author": {
          "name": "{{ Globals.bot.name }} logs"
        },
        "timestamp": null,
        "color": "#ccffff"
      }
    }
  },
  "name": "nano-template",
  "author": "ghom",
  "license": "ISC",
  "version": "v1.4.0-alpha",
  "description": "Discord bot in TypeScript.",
  "main": "dist/index.js",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false,
    "useTabs": false
  },
  "scripts": {
    "prettier": "prettier src --write",
    "build": "git rm -r --ignore-unmatch -f ./dist && npm run prettier && tsc",
    "start": "npm run build && node .",
    "push": "npm run prettier && git add * && git commit -m continue && git push origin master"
  },
  "keywords": [
    "discord.js",
    "bot",
    "typescript",
    "handler",
    "command-handler"
  ],
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/ws": "^7.2.6",
    "prettier": "^2.0.5",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "better-sqlite-pool": "0.3.0",
    "better-sqlite3": "7.1.0",
    "chalk": "4.1.0",
    "discord.js": "12.3.1",
    "dotenv": "8.2.0",
    "enmap": "5.6.1",
    "querystring": "0.2.0"
  }
}
